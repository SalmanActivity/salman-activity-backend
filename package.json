{
  "name": "salman-activity-backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "tsc",
    "start": "npm run build && node ./dist/app.js",
    "seed-dev": "DEBUG=salman-activity-backend:seed node_modules/.bin/ts-node ./scripts/seed_db.ts",
    "dev": "DEBUG=salman-activity-backend:server ENV=dev nodemon node_modules/.bin/ts-node app.ts",
    "test": "node node_modules/.bin/mocha -r ts-node/register ./**/*.spec.ts",
    "pretest-integration": "npm run seed-dev",
    "test-integration": "node node_modules/.bin/mocha --exit -r ts-node/register -t 2000 ./**/*.test.ts",
    "encrypt-config": "node ./scripts/configEncrypt.js"
  },
  "engineStrict": true,
  "engines": {
    "node": ">= 6.0.0"
  },
  "dependencies": {
    "async": "^2.6.0",
    "bluebird": "^3.5.1",
    "body-parser": "~1.13.2",
    "cookie-parser": "~1.3.5",
    "express": "~4.13.1",
    "joi": "^13.1.2",
    "jsonwebtoken": "^8.1.1",
    "meanie-mongoose-to-json": "^1.3.0",
    "mongoose": "^5.0.7",
    "password-hash": "^1.2.2"
  },
  "devDependencies": {
    "@types/body-parser": "^1.16.8",
    "@types/chai": "^4.1.2",
    "@types/mocha": "^5.0.0",
    "@types/mongoose": "^5.0.7",
    "@types/sinon": "^4.3.1",
    "chai": "^4.1.2",
    "mocha": "^5.0.5",
    "nodemon": "^1.15.1",
    "sinon": "^4.4.3",
    "supertest": "^3.0.0",
    "ts-node": "^5.0.1",
    "typescript": "^2.8.1"
  }
}
